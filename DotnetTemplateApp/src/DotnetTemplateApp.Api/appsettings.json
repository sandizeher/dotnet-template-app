{
  "AccessOptions": {
    "MobileApiKey": "<SECRET>",
    "WebApiKey": "<SECRET>"
  },
  "JwtOptions": {
    "Issuer": "template-app.com",
    "Audience": "template-app.com",
    "SecretKey": "<SECRET>",
    "Kid": "<SECRET>",
    "AccessTokenTTLMinutes": 60,
    "RefreshTokenTTLDays": 2
  },
  "DbSettings": {
    "ConnectionString": "<SECRET>"
  },
  "RedisSettings": {
    "ConnectionString": "<SECRET>"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "RateLimitingSettings": {
    "DefaultRateLimitWindowInSeconds": 5,
    "DefaultWindowRateLimit": 150
  },
  "AuthenticationSettings": {
    "LoginAttempts": 5,
    "CooldownTimeInMinutes": 15
  }
}
